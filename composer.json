{
    "name": "yuk1/laravel-repository",
    "description": "This is the command to create a Laravel repository.",
    "license": "MIT",
    "authors": [
        {
            "name": "YuK1Game",
            "email": "develop@yuk1.jp"
        }
    ],
    "homepage": "https://github.com/yuk1/laravel-repository",
    "keywords": ["Laravel", "Repository"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "YuK1\\LaravelRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YuK1\\LaravelRepository\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YuK1\\LaravelRepository\\LaravelRepositoryServiceProvider"
            ],
            "aliases": {
                "LaravelRepository": "YuK1\\LaravelRepository\\Facades\\LaravelRepository"
            }
        }
    }
}
